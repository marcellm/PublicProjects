<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ConstruirMenuSimples.Home.Master"
             Title="Home"
             Icon="menu.png">
    <ContentPage.Content>
        <StackLayout>
            
            <StackLayout Padding="10" Orientation="Vertical" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
                <Button Text="Cadastro" Clicked="menu_acessar_cadastro_clicked" BackgroundColor="DodgerBlue" HorizontalOptions="FillAndExpand" VerticalOptions="StartAndExpand" TextColor="White" />
                <Button Text="Sair" Clicked="sair_clicked" BackgroundColor="DodgerBlue" HorizontalOptions="FillAndExpand" VerticalOptions="EndAndExpand" TextColor="White" />
            </StackLayout>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>